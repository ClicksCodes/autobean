2000-01-01 custom "autobean.share.policy" "default"
    share-Alice: 1
    share-Bob: 1

2000-01-01 open Assets:BankOfBean:JointAccount
2000-01-01 open Expenses:Bean
2000-01-01 open Liabilities:Payable
2000-01-01 open Assets:Receivable:Alice
2000-01-01 open Assets:Receivable:Bob
2000-01-01 open Assets:Receivable:Charlie

2000-01-01 *
    Assets:BankOfBean:JointAccount           -100.00 USD
    Expenses:Bean                             100.00 USD
        share-Alice: 1

2000-01-02 *
    Assets:BankOfBean:JointAccount           -100.00 USD
    Expenses:Bean                             100.00 USD
        share-Bob: 2
        share-Charlie: 3

2000-01-03 *
    Assets:BankOfBean:JointAccount           -100.00 USD
    Expenses:Bean                             100.00 USD
        share-Charlie: 1

2000-01-04 *
    Assets:BankOfBean:JointAccount           -100.00 USD
    Expenses:Bean                              20.00 USD
        share-Alice: 1
    Expenses:Bean                              80.00 USD
        share-Charlie: 1
